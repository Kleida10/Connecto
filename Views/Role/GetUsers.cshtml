@{
    ViewBag.Title = ViewBag.Role + " Users";
}

<h2>@ViewBag.Title</h2>
<hr />
<table class="table table-hover">
    @foreach (var user in ViewBag.Users)
    {
        <tr class="table-primary">
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.UserName</td>
        </tr>
    }
</table>