@{
    ViewBag.Title =ViewBag.Title + "'s Parents";
}
<hr/>
<h2>@ViewBag.Title</h2>
<hr />
<table class="table table-hover">
    <tr class="table-primary">
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    @foreach (var user in ViewBag.StParents)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.UserName</td>
        </tr>
    }
</table>

